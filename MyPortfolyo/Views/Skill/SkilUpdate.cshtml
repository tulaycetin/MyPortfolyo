@model Skill

@{
    ViewData["Title"] = "SkilUpdate";
    Layout = "~/Views/Layout/Index.cshtml";
}





<div class="content">
    <div class="container-fluid">
        <h4 class="page-title">Deneyimler</h4>
        <div class="row">
            <div class="col-md-12">
                <div class="card">
                    <div class="card-header">
                        <div class="card-title">
                            Deneyim Güncelleme Sayfası
                        </div>
                    </div>
                    <div class="card-body">

                        <form method="post">
                            <input type="hidden" asp-for="SkillId" />

                            <label>Yetenek</label>
                            <input type="text" asp-for="Title" class="form-control " />
                            <br />
                            <label>Derece</label>
                            <input type="text" asp-for="Value" class="form-control " />
                            <br />
                         

                            <button class="btn btn-primary">Yetenekleri Güncelle</button>
                        </form>

                    </div>
                </div>

            </div>

        </div>
    </div>
</div>



 